{
	"name": "core-old",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/old/core-old/src",
	"prefix": "platform",
	"tags": [],
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/angular:package",
			"outputs": ["{workspaceRoot}/dist/{projectRoot}"],
			"options": {
				"project": "libs/old/core-old/ng-package.json"
			},
			"configurations": {
				"production": {
					"tsConfig": "libs/old/core-old/tsconfig.lib.prod.json"
				},
				"development": {
					"tsConfig": "libs/old/core-old/tsconfig.lib.json"
				}
			},
			"defaultConfiguration": "production"
		},
		"package": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"pnpm exec nx build core",
					"pnpm exec nx build-ecosystem core",
					"pnpm exec nx build plugin",
					"pnpm exec nx generate-json core"
				],
				"parallel": false
			}
		},
		"build-ecosystem": {
			"executor": "nx:run-commands",
			"options": {
				"commands": ["pnpm exec nx build soba", "pnpm exec nx build cannon", "pnpm exec nx build postprocessing"],
				"parallel": true
			}
		},
		"generate-json": {
			"executor": "nx:run-commands",
			"options": {
				"commands": ["node ./tools/scripts/generate-json.mjs", "node ./tools/scripts/generate-soba-json.mjs"],
				"parallel": true
			}
		},
		"publish": {
			"command": "npm publish",
			"options": {
				"cwd": "dist/libs/old/core-old"
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/old/core-old/jest.config.ts"
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		}
	}
}
