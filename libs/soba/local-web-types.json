{
  "$schema": "https://raw.githubusercontent.com/JetBrains/web-types/master/schema/web-types.json",
  "version": "2.0",
  "name": "angular-three",
  "framework": "angular",
  "js-types-syntax": "typescript",
  "framework-config": {
    "enable-when": {
      "node-packages": ["angular-three-soba"]
    }
  },
  "contributions": {
    "html": {
      "elements": [
        {
          "name": "ngt-soft-shadow-material",
          "extends": {
            "path": "/html/elements/ngt-shader-material"
          },
          "attributes": [
            {
              "name": "color"
            },
            {
              "name": "[color]"
            },
            {
              "name": "[blend]"
            },
            {
              "name": "[map]"
            }
          ]
        }
      ]
    }
  }
}
